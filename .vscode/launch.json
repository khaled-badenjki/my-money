  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Allocate",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.js",
      "args": ["allocate", "6000", "3000", "1000"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Tests",
      "runtimeExecutable": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["--timeout", "999999", "--colors", "${workspaceFolder}/tests/**/*.js"]
    }
  ]
}